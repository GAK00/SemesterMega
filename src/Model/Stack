#ifndef MODEL_STACK_
#define MODEL_STACK_
#include "DoubleList.hpp"
template<class Type>
class Stack : DoubleList<Type>
{
private:
public:
    Stack();
    void add(Type value);
};

/*
 the add method only adds to the end
 */
template <class Type>
void Stack<Type> :: add(Type value) : DoubleList<Type> :: add(value)
    {

    }



#endif /* MODEL_STACK_ */
